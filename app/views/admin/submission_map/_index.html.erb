<script src="//maps.google.com/maps/api/js?key=<%= ENV['GOOGLE_API_KEY'] %>"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
<div class="col-lg-12">
  <div class="map">
    <%= render partial: 'map', locals: {submissions: []} %>
  </div>
  <br>
  <div class="form-group.col-lg-6">
    <span><b>Select Categories</b></span>
    <%= select_tag('category_ids', options_for_select(categories), multiple: true) %>
    <input type="button" value='Search' onclick="fetch_near_by_submissions();"/>
  </div>
  <div class="modal"></div>

</div>
